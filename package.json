{
  "name": "remove-release-asset",
  "version": "1.0.0",
  "private": true,
  "description": "Delete a release asset from an existing GitHub Release",
  "main": "lib/index.js",
  "scripts": {
    "bundle": "npm run format:write && npm run package",
    "build": "tsc",
    "format:write": "npx prettier --write **/*.ts",
    "format:check": "npx prettier --check **/*.ts",
    "lint": "npx eslint .",
    "package": "npx rimraf ./dist && npx rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "package:watch": "npm run package -- --watch",
    "test": "npx jest",
    "all": "npm run format:write && npm run lint && npm run test && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flcdrg/remove-release-asset-action"
  },
  "keywords": [
    "actions",
    "node"
  ],
  "author": "flcdrg",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^3.0.0",
    "@actions/exec": "^3.0.0",
    "@actions/github": "^9.0.0",
    "minimatch": "^10.2.4",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.4",
    "@eslint/js": "^9.39.2",
    "@octokit/types": "^16.0.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.4",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^10.0.2",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-jest": "^29.15.0",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "jest-circus": "^30.2.0",
    "js-yaml": "^4.1.1",
    "prettier": "^3.8.1",
    "rollup": "^4.59.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=24.0.0 <25.0.0"
  }
}
